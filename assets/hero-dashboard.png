<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="cardGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f1f5f9;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="accentGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6366f1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bgGradient)"/>
  
  <!-- Dashboard Container -->
  <rect x="50" y="50" width="700" height="500" rx="20" fill="url(#cardGradient)" stroke="#e2e8f0" stroke-width="2"/>
  
  <!-- Header Bar -->
  <rect x="50" y="50" width="700" height="60" rx="20" fill="url(#accentGradient)" opacity="0.1"/>
  <rect x="70" y="70" width="20" height="20" rx="10" fill="url(#accentGradient)" opacity="0.3"/>
  <rect x="100" y="75" width="100" height="10" rx="5" fill="url(#accentGradient)" opacity="0.5"/>
  
  <!-- Navigation -->
  <rect x="600" y="75" width="60" height="10" rx="5" fill="#6b7280" opacity="0.3"/>
  <rect x="670" y="75" width="60" height="10" rx="5" fill="#6b7280" opacity="0.3"/>
  
  <!-- Main Content Area -->
  <!-- Stats Cards -->
  <rect x="70" y="140" width="150" height="100" rx="10" fill="white" stroke="#e2e8f0"/>
  <rect x="240" y="140" width="150" height="100" rx="10" fill="white" stroke="#e2e8f0"/>
  <rect x="410" y="140" width="150" height="100" rx="10" fill="white" stroke="#e2e8f0"/>
  <rect x="580" y="140" width="150" height="100" rx="10" fill="white" stroke="#e2e8f0"/>
  
  <!-- Chart Area -->
  <rect x="70" y="260" width="320" height="200" rx="10" fill="white" stroke="#e2e8f0"/>
  <rect x="410" y="260" width="320" height="200" rx="10" fill="white" stroke="#e2e8f0"/>
  
  <!-- Chart Lines -->
  <polyline points="90,380 140,350 190,320 240,340 290,310 340,290 370,280" 
            fill="none" stroke="url(#accentGradient)" stroke-width="3" opacity="0.8"/>
  
  <!-- AI Elements -->
  <circle cx="500" cy="350" r="30" fill="url(#accentGradient)" opacity="0.1"/>
  <circle cx="500" cy="350" r="20" fill="none" stroke="url(#accentGradient)" stroke-width="2" opacity="0.6"/>
  
  <!-- Neural Network -->
  <g transform="translate(480, 330)">
    <circle cx="10" cy="10" r="3" fill="url(#accentGradient)"/>
    <circle cx="30" cy="5" r="3" fill="url(#accentGradient)"/>
    <circle cx="30" cy="25" r="3" fill="url(#accentGradient)"/>
    <circle cx="20" cy="40" r="3" fill="url(#accentGradient)"/>
    
    <line x1="10" y1="10" x2="30" y2="5" stroke="url(#accentGradient)" stroke-width="1" opacity="0.6"/>
    <line x1="10" y1="10" x2="30" y2="25" stroke="url(#accentGradient)" stroke-width="1" opacity="0.6"/>
    <line x1="30" y1="5" x2="20" y2="40" stroke="url(#accentGradient)" stroke-width="1" opacity="0.6"/>
    <line x1="30" y1="25" x2="20" y2="40" stroke="url(#accentGradient)" stroke-width="1" opacity="0.6"/>
  </g>
  
  <!-- Content Generation Illustration -->
  <rect x="450" y="300" width="80" height="4" rx="2" fill="url(#accentGradient)" opacity="0.7"/>
  <rect x="450" y="310" width="120" height="4" rx="2" fill="url(#accentGradient)" opacity="0.5"/>
  <rect x="450" y="320" width="100" height="4" rx="2" fill="url(#accentGradient)" opacity="0.3"/>
  
  <!-- Floating Elements -->
  <circle cx="650" cy="180" r="8" fill="#10b981" opacity="0.7"/>
  <rect x="680" y="200" width="30" height="15" rx="7" fill="#f59e0b" opacity="0.7"/>
  <polygon points="600,480 610,460 620,480" fill="#ef4444" opacity="0.7"/>
  
  <!-- Bottom Progress Bar -->
  <rect x="70" y="480" width="660" height="8" rx="4" fill="#e2e8f0"/>
  <rect x="70" y="480" width="420" rx="4" height="8" fill="url(#accentGradient)" opacity="0.7"/>
  
  <!-- Text Labels (simulated) -->
  <rect x="90" y="160" width="80" height="8" rx="4" fill="#6b7280" opacity="0.4"/>
  <rect x="90" y="175" width="40" height="15" rx="7" fill="url(#accentGradient)" opacity="0.8"/>
  
  <rect x="260" y="160" width="70" height="8" rx="4" fill="#6b7280" opacity="0.4"/>
  <rect x="260" y="175" width="60" height="15" rx="7" fill="#10b981" opacity="0.8"/>
  
  <rect x="430" y="160" width="90" height="8" rx="4" fill="#6b7280" opacity="0.4"/>
  <rect x="430" y="175" width="50" height="15" rx="7" fill="#f59e0b" opacity="0.8"/>
  
  <rect x="600" y="160" width="85" height="8" rx="4" fill="#6b7280" opacity="0.4"/>
  <rect x="600" y="175" width="45" height="15" rx="7" fill="#ef4444" opacity="0.8"/>
</svg>